[run]
source = web/dorashop
omit = 
    */venv/*
    */virtualenv/*
    */__pycache__/*
    */migrations/*
    */staticfiles/*
    */media/*
    manage.py
    */settings/*
    */wsgi.py
    */asgi.py
    */tests/*
    */test_*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\(Protocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml